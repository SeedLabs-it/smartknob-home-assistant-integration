{
  "name": "@twind/with-react",
  "version": "1.1.3",
  "description": "Seamless integration of Twind in a React project",
  "type": "module",
  "homepage": "https://twind.style",
  "bugs": {
    "url": "https://github.com/tw-in-js/twind/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tw-in-js/twind.git",
    "directory": "packages/with-react"
  },
  "keywords": [
    "twind",
    "tw-in-js",
    "tailwind",
    "tailwindcss",
    "tailwind-in-js",
    "twind-with"
  ],
  "license": "MIT",
  "author": {
    "name": "Sascha Tandel",
    "url": "https://github.com/sastan"
  },
  "funding": [
    {
      "type": "Open Collective",
      "url": "https://opencollective.com/twind"
    },
    {
      "type": "Github Sponsor",
      "url": "https://github.com/sponsors/tw-in-js"
    },
    {
      "type": "Ko-fi",
      "url": "https://ko-fi.com/twind"
    }
  ],
  "engines": {
    "node": ">=14.15.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./with-react.cjs",
  "exports": {
    ".": {
      "types": "./with-react.d.ts",
      "esnext": "./with-react.esnext.js",
      "deno": "./with-react.deno.js",
      "module": "./with-react.js",
      "worker": "./with-react.worker.js",
      "browser": "./with-react.browser.js",
      "script": "./with-react.global.js",
      "node": {
        "import": "./with-react.mjs",
        "require": "./with-react.cjs"
      },
      "default": "./with-react.js"
    },
    "./inline": {
      "types": "./inline.d.ts",
      "esnext": "./inline.esnext.js",
      "deno": "./inline.deno.js",
      "module": "./inline.js",
      "worker": "./inline.worker.js",
      "browser": "./inline.browser.js",
      "script": "./inline.global.js",
      "node": {
        "import": "./inline.mjs",
        "require": "./inline.cjs"
      },
      "default": "./inline.js"
    },
    "./pipeableStream": {
      "types": "./pipeableStream.d.ts",
      "esnext": "./pipeableStream.esnext.js",
      "module": "./pipeableStream.js",
      "node": {
        "import": "./pipeableStream.mjs",
        "require": "./pipeableStream.cjs"
      },
      "default": "./pipeableStream.js"
    },
    "./readableStream": {
      "types": "./readableStream.d.ts",
      "esnext": "./readableStream.esnext.js",
      "deno": "./readableStream.deno.js",
      "module": "./readableStream.js",
      "worker": "./readableStream.worker.js",
      "browser": "./readableStream.browser.js",
      "script": "./readableStream.global.js",
      "node": {
        "import": "./readableStream.mjs",
        "require": "./readableStream.cjs"
      },
      "default": "./readableStream.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@twind/core": "^1.1.0",
    "react": "^16.6.0 || 17.x || 18.x",
    "react-dom": "^16.6.0 || 17.x || 18.x",
    "typescript": "^4.8.4"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "fast-diff": "^1.2.0"
  },
  "module": "./with-react.js",
  "unpkg": "./with-react.global.js",
  "jsdelivr": "./with-react.global.js",
  "types": "./with-react.d.ts"
}